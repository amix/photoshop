			BHI.S		@2
			CMP.W		#3796,D0
			BLE.S		@1
			MOVE.W		#3796,D0

;			Look up in second table

@1			LEA 		@table2,A0
			MOVE.B		(A0,D0.W),@result+1(A6)

;			Clean up and exit

@2			UNLK		A6
			MOVE.L		(SP)+,A0
			ADD.W		#@params,SP
			JMP 		(A0)

